    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "${workspaceFolder}/main.py",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--dataset",
                "bundle",
                "--batch_size",
                "512",
                "--lr",
                "1e-3",
                "--transformer_num",
                "2",
                "--embedding",
                "8",
                "--num_head",
                "8",
                "--time_aware",
                "True",
                "--seed",
                "0",
                "--data_path",
                "./data/bundle_time",
                "--max_len",
                "8",
                "--force_restart"
            ]
        }
    ]
}